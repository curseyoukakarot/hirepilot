        "nanoid": "5.1.6",
        "node-cron": "4.2.1",
        "node-fetch": "2.6.11",
        "nodemailer": "^6.10.1",
        "openai": "^4.96.0",
        "p-map": "5.1.0",
        "pg": "^8.11.1",
        "pino": "9.13.1",
        "playwright": "^1.44.1",
