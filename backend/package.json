        "dayjs": "1.11.13",
        "dayjs-business-days": "1.0.4",
        "dockerode": "4.0.9",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-list-endpoints": "^6.0.0",
        "express-validator": "^7.2.1",
        "ghost-cursor": "1.4.1",
