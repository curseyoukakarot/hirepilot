{
    "name": "hirepilot-backend",
    "version": "1.0.0",
    "main": "dist/index.js",
    "private": true,
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only --require dotenv/config server.ts",
        "worker:ingest": "ts-node workers/ingestPhantom.ts",
        "worker:enrich": "ts-node workers/enrichLeads.ts",
        "worker:email": "ts-node workers/emailQueueWorker.ts",
        "worker:campaign": "ts-node workers/campaignQueueWorker.ts",
        "worker:attribution": "ts-node workers/emailAttributionWorker.ts",
        "worker:backfill": "ts-node workers/emailAttributionBackfill.ts",
        "worker:attribution-standalone": "node dist/workers/standalone/emailAttributionStandalone.js",
        "worker:attribution-standalone-dev": "ts-node workers/standalone/emailAttributionStandalone.ts",
        "test:puppet": "ts-node scripts/testPuppetSystem.ts",
        "test:linkedin": "ts-node scripts/testLinkedInConnection.ts",
        "test:proxy": "ts-node scripts/testProxyIntegration.ts",
        "test:behavior": "ts-node scripts/testHumanBehavior.ts",
        "cron": "ts-node scripts/cronJobRunner.ts",
        "sniper:watchdog": "ts-node scripts/sniperWatchdog.ts",
        "test:cron": "ts-node scripts/testCronJobRunner.ts",
        "test:rex": "ts-node scripts/testRexIntegration.ts",
        "test:admin": "ts-node scripts/testAdminDashboard.ts",
        "test:linkedin-modal": "ts-node scripts/testLinkedInModalEnhancement.ts",
        "test:proxy-assignment": "ts-node scripts/testProxyAssignmentSystem.ts",
        "test:proxy-health": "ts-node scripts/testProxyHealthMonitoring.ts",
        "test:sourcing": "ts-node scripts/testSourcingAPI.ts",
        "test:sendgrid-inbound": "ts-node scripts/testSendgridInbound.ts",
        "test:rex-sourcing": "ts-node scripts/testRexSourcing.ts",
        "test:rex-orchestrator": "ts-node scripts/testRexOrchestrator.ts",
        "test:sourcing-curl": "./scripts/testSourcingAPI_curl.sh",
        "test:notifications": "ts-node scripts/testNotificationsAPI.ts",
        "build": "tsc --incremental",
        "build:fast": "tsc --incremental --skipLibCheck --noEmit false",
        "build:production": "tsc --skipLibCheck --incremental false --sourceMap false --declaration false",
        "build:ultra-fast": "tsc --skipLibCheck --noImplicitAny false --noEmitOnError false --sourceMap false --declaration false --incremental false",
        "build:clean": "rm -rf dist .tsbuildinfo && tsc --incremental",
        "start": "node dist/server.js",
        "_postinstall_disabled": "npm run build:ultra-fast",
        "backfill-credits": "ts-node scripts/backfillUserCredits.ts",
        "backfill-activate-campaigns": "ts-node scripts/backfillActivateCampaigns.ts",
        "_postinstall_note": "Playwright browsers installed via Dockerfile",
        "test": "jest",
        "test:free-cadence": "ts-node scripts/testFreeForeverCadence.ts"
    },
    "puppeteer": {
        "skipDownload": true
    },
    "dependencies": {
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@modelcontextprotocol/sdk": "^1.13.1",
