{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "outDir": "dist",
      "rootDir": ".",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": false,
      "noUnusedLocals": false,
      "noUnusedParameters": false
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  